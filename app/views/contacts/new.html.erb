<%= form_for @contact do |f| %>
  <%#= f.error_messages %>
  <%= f.hidden_field :user_id %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </p>

  <p><%= f.submit %></p>
<% end %>
